{
	"radio": {
		"model": 1041,
		"device": "/dev/ttyUSB0",
		"rate": 4800
	},
	"output":{
		"filename": "./out/noise_profile.csv",
		"filemode": "a",
		"columns": ["sweep", "timestamp", "band_name", "frequency", "signal_level"]
